package miniProjeAsalSayi;

public class Main {

	public static void main(String[] args) {

		int sayi = 1;
		boolean asal = true;

		if (sayi == 1) {
			System.out.println("ASAL DEGILDIR !");
			return;
		}

		if (sayi > 2) {

			for (int i = 2; i < sayi; i++) {

				if (sayi % 2 == 0) {
					asal = false;
				}
			}
			if (asal) {
				System.out.println("ASAL !");
			} else {
				System.out.println("ASAL DEGILDIR !");
			}
		}
		System.out.println("GECERSIZ SAYI ! ");
	}

}
