package loopDemo;

//for dongusu ve while dongusu

public class Main {

	public static void main(String[] args) {

		for (int i = 1; i < 10; i += 2) {
			System.out.println(i);
		}

		System.out.println("Dongu Bitti ! ");

		int i = 0;
		//while
		while(i<10) {
			System.out.println(i);
			i++;
		}
	}
}
