package reCapDemo2;

//komple tekrar 1 

public class Main {

	public static void main(String[] args) {
		
		// double[] myList = new double[4]; bu sekilde bir dizide kullanilabilir.
		
		double[] myList = {1.2,1.3,4.3,5.6};
		double total =0;
		for(double number:myList) {
			
			total=total+number;
			System.out.println(number);
		}
		System.out.println("Toplam = " + total);
		
		

	}

}
