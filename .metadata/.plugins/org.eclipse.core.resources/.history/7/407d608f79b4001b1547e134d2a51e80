package miniProjeMukemmelSayi;

public class Main {

	public static void main(String[] args) {

		// 6 --> 1+2+3 = 6 ise mukemmel bir sayidir !

		int number = 6;
		int total = 0;
		for (int i = 0; i < number; i++) {

			if(number%i ==0) {
				total+=i;
			}
			
		}
		System.out.println("Toplam = " + total);

	}

}
